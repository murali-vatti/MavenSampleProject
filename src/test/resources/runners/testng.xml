<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Data Driven Basics">

	<!-- LISTERNERS BEGIN -->
	<listeners>
		<!-- ReportNG Listeners Begin -->
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<!-- ReportNG Listeners End -->

		<!-- Custom Listeners Begin -->
		<listener class-name="com.murali.listeners.CustomListeners" />
		<!-- Custom Listeners End -->
	</listeners>
	<!-- LISTERNERS END -->

	<test name="Bank Manager Login Test">
		<classes>
			<class name="com.murali.testcases.BankManagerLoginTest" />
		</classes>
	</test> <!-- Test -->

	<test name="Add Customer Test">
		<classes>
			<class name="com.murali.testcases.AddCustomerTest" />
		</classes>
	</test> <!-- Test -->
	
	<test name="Open Account Test">
		<classes>
			<class name="com.murali.testcases.OpenAccountTest" />
		</classes>
	</test> <!-- Test -->
	
	
	


</suite> <!-- Suite -->
